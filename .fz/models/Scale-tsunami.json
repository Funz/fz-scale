{
    "id": "Scale-tsunami",
    "varprefix": "&",
    "formulaprefix": "@",
    "delim": "()",
    "commentline": "'",
    "output": {
        "mean_keff": "grep 'best estimate system k-eff' *.out | head -1 | tr -s '[:space:]' | cut -d ' ' -f7",
        "sigma_keff":  "grep 'best estimate system k-eff' *.out | head -1 | tr -s '[:space:]' | cut -d ' ' -f11",
        "mean_E_lethargy": "grep 'Energy of average lethargy of Fission' *.out | head -1 | awk '{print $7}' 2>/dev/null",
        "sigma_E_lethargy": "grep 'Energy of average lethargy of Fission' *.out | head -1 | awk '{print $10}' 2>/dev/null",
        "mean_nubar": "grep 'system nu bar' *.out | head -1 | awk '{print $4}' 2>/dev/null",
        "sigma_nubar": "grep 'system nu bar' *.out | head -1 | awk '{print $7}' 2>/dev/null",
        "mean_free_path": "grep 'system mean free path' *.out | head -1 | awk '{print $5}' 2>/dev/null",
        "sigma_free_path": "grep 'system mean free path' *.out | head -1 | awk '{print $8}' 2>/dev/null",
        "mean_sens_h1_total": "grep ' h-1          total' *.out | head -1 | awk '{print $3}' 2>/dev/null ",
        "sigma_sens_h1_total": "grep ' h-1          total' *.out | head -1 | awk '{print $5}' 2>/dev/null ",
        "mean_sens_h1_scatter": "grep ' h-1          scatter' *.out | head -1 | awk '{print $3}' 2>/dev/null ",
        "sigma_sens_h1_scatter": "grep ' h-1          scatter' *.out | head -1 | awk '{print $5}' 2>/dev/null ",
        "mean_sens_h1_capture": "grep ' h-1          capture' *.out | head -1 | awk '{print $3}' 2>/dev/null ",
        "sigma_sens_h1_capture": "grep ' h-1          capture' *.out | head -1 | awk '{print $5}' 2>/dev/null ",
        "mean_sens_u235_total": "grep ' u-235        total' *.out | head -1 | awk '{print $3}' 2>/dev/null ",
        "sigma_sens_u235_total": "grep ' u-235        total' *.out | head -1 | awk '{print $5}' 2>/dev/null ",
        "mean_sens_u235_fission": "grep ' u-235        fission' *.out | head -1 | awk '{print $3}' 2>/dev/null ",
        "sigma_sens_u235_fission": "grep ' u-235        fission' *.out | head -1 | awk '{print $5}' 2>/dev/null ",
        "mean_sens_u238_total": "grep ' u-238        total' *.out | head -1 | awk '{print $3}' 2>/dev/null ",
        "sigma_sens_u238_total": "grep ' u-238        total' *.out | head -1 | awk '{print $5}' 2>/dev/null "
    }
}
